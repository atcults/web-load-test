    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Basic",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\dist\\basic.js",
            "args": [
                "--port=9001" ,"--interval=1000"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Web",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\dist\\web.js",
            "args": [
                "--port=9001" ,"--delay=50"
            ]
        }
    ]
}